{
  "id": "@@JOB_NAME@@",
  "description": "@@JOB_NAME@@",
  "labels": {
    "owner": "@@OWNER@@"
  },
  "run": {
      "cpus": 0.5,
      "mem": 512,
      "disk": 0,
      "restart": {
          "policy": "NEVER",
          "activeDeadlineSeconds": 0
      },
      "docker": {
          "image": "mastodonc/kixi.event-replay:@@VERSION@@"
      },
      "env": {
          "START_TIME": "@@START_TIME@@",
          "END_TIME": "@@END_TIME@@",
          "TARGET_STREAM": "@@TARGET_STREAM@@",
          "KINESIS_ENDPOINT": "@@KINESIS_ENDPOINT@@",
          "REGION": "@@REGION@@",
          "S3_BASE_DIR": "@@S3_BASE_DIR@@"
      }
  }
}
